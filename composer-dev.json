{
	"name": "buepro/typo3-user-pizpalue",
	"type": "typo3-cms-extension",
	"description": "TYPO3 Custom template being used with template pizpalue.",
	"keywords": [
		"TYPO3 CMS",
		"bootstrap",
		"pizpalue",
		"responsive",
		"frontend",
		"theme",
		"web"
	],
	"license": "GPL-2.0-or-later",
	"authors": [
		{
			"name": "Roman BÃ¼chler",
			"email": "rb@buechler.pro",
			"homepage": "https://www.buechler.pro/",
			"role": "Developer"
		}
	],
	"require-dev": {
		"friendsofphp/php-cs-fixer": "^2.16.3"
	},
	"prefer-stable": true,
	"support": {
		"issues": "https://github.com/buepro/typo3-user_pizpalue/issues",
		"source": "https://github.com/buepro/typo3-user_pizpalue"
	},
	"scripts": {
		"fix:php:cs:dry": "./vendor/friendsofphp/php-cs-fixer/php-cs-fixer fix --config .php_cs.php -v --dry-run --using-cache false --diff --diff-format=udiff",
		"fix:php:cs": "./vendor/friendsofphp/php-cs-fixer/php-cs-fixer fix --config .php_cs.php -v --using-cache false",
		"check:php": "@fix:php:cs:dry",
		"fix:php": "@fix:php:cs",
		"uninstall": "rm -rf composer-dev.lock vendor && ls -l"
	}
}
