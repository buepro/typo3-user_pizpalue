{
	"name": "buepro/typo3-user-pizpalue",
	"description": "TYPO3 site package being used with template pizpalue.",
	"license": "GPL-2.0-or-later",
	"type": "typo3-cms-extension",
	"keywords": [
		"TYPO3 CMS",
		"bootstrap",
		"pizpalue",
		"responsive",
		"frontend",
		"theme",
		"web"
	],
	"authors": [
		{
			"name": "Roman BÃ¼chler",
			"email": "rb@buechler.pro",
			"homepage": "https://www.buechler.pro/",
			"role": "Developer"
		}
	],
	"support": {
		"issues": "https://github.com/buepro/typo3-user_pizpalue/issues",
		"source": "https://github.com/buepro/typo3-user_pizpalue"
	},
	"require": {
		"b13/container": "^2.1.3 || dev-master",
		"buepro/typo3-container-elements": "^5.0.0",
		"buepro/typo3-easyconf": "^2.0.0",
		"buepro/typo3-pizpalue": "^15.0",
		"buepro/typo3-pizpalue-distribution": "^6.0.0",
		"helhum/typo3-console": "dev-main",
		"typo3/cms-base-distribution": "^12.2 || dev-main",
		"typo3/cms-core": "^12.2 || dev-main",
		"typo3/cms-indexed-search": "^12.2 || dev-main",
		"typo3/cms-lowlevel": "^12.2 || dev-main",
		"typo3/cms-recycler": "^12.2 || dev-main",
		"typo3/cms-redirects": "^12.2 || dev-main"
	},
	"require-dev": {
		"ergebnis/composer-normalize": "^2.15.0",
		"friendsofphp/php-cs-fixer": "^3.0.0",
		"friendsoftypo3/phpstan-typo3": "^0.9.0",
		"helmich/typo3-typoscript-lint": "^3.0.0",
		"overtrue/phplint": "^5.3.0 || ^6.1.0",
		"phpspec/prophecy-phpunit": "^2.0",
		"phpstan/phpstan": "^1.8.5",
		"phpstan/phpstan-deprecation-rules": "^1.0.0",
		"phpstan/phpstan-phpunit": "^1.1.1",
		"phpstan/phpstan-strict-rules": "^1.4.3",
		"seld/jsonlint": "^1.9",
		"typo3/testing-framework": "7.x-dev"
	},
	"repositories": {
		"pizpalue": {
			"type": "path",
			"url": "/mnt/public/package/typo3-pizpalue"
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"autoload": {
		"psr-4": {
			"Buepro\\UserPizpalue\\": "Classes"
		}
	},
	"config": {
		"allow-plugins": {
			"typo3/class-alias-loader": true,
			"typo3/cms-composer-installers": true,
			"ergebnis/composer-normalize": true
		},
		"bin-dir": ".build/bin",
		"optimize-autoloader": true,
		"sort-packages": true,
		"vendor-dir": ".build/vendor"
	},
	"extra": {
		"branch-alias": {
			"dev-main": "5.0.x-dev"
		},
		"typo3/cms": {
			"extension-key": "user_pizpalue",
			"web-dir": ".build/public"
		}
	},
	"scripts": {
		"post-autoload-dump": [
			"[ -d .build/public/_assets ] || mkdir -p .build/public/typo3conf/ext/",
			"[ -d .build/public/_assets ] || [ -L .build/public/typo3conf/ext/user_pizpalue ] || ln -snvf ../../../../. .build/public/typo3conf/ext/user_pizpalue",
			"[ -d .build/public/_assets ] || mkdir -p .build/config/sites/development",
			"[ -d .build/public/_assets ] || [ -L .build/config/sites/development/config.yaml ] || ln -svf ../../../../Build/site/config.yaml .build/config/sites/development/config.yaml",
			"[ ! -d .build/public/_assets ] || mkdir -p config/sites/development",
			"[ ! -d .build/public/_assets ] || [ -L config/sites/development/config.yaml ] || ln -svf ../../../Build/site/config.yaml config/sites/development/config.yaml"
		],
		"ci": [
			"@ci:static"
		],
		"ci:composer:normalize": "@composer normalize --dry-run",
		"ci:json:lint": "find . ! -path './.build/*' ! -path './Build/node_modules' ! -name '*.preprocessed.json' -name '*.json' | xargs php ./.build/bin/jsonlint -q",
		"ci:php:cs": [
			"php-cs-fixer --diff -v --dry-run fix"
		],
		"ci:php:lint": [
			"phplint"
		],
		"ci:php:stan": [
			"phpstan analyze --configuration ./Build/phpstan.neon"
		],
		"ci:static": [
			"@ci:composer:normalize",
			"@ci:json:lint",
			"@ci:php:cs-fixer",
			"@ci:php:lint",
			"@ci:php:stan"
		],
		"ddev:install": [
			"ddev start",
			"ddev composer u",
			"ddev restart",
			"ddev composer site:install",
			"ddev launch typo3",
			"echo 'INFO: Login to BE using admin/admin'"
		],
		"ddev:uninstall": [
			"ddev composer typo3cms:database:export",
			"ddev delete",
			"rm -rf .build/",
			"git checkout '.build/'"
		],
		"finalize-installation": [
			"composer u",
			".build/bin/typo3cms database:updateschema"
		],
		"fix": [
			"@fix:php:cs"
		],
		"fix:composer:normalize": "@composer normalize",
		"fix:php:cs": [
			"php-cs-fixer --diff -v fix"
		],
		"site:install": [
			"cp Build/site/config.yaml .build/config/sites/default/config.yaml",
			"@typo3cms:install:setup",
			".build/bin/typo3 extension:setup",
			"@typo3cms:database:import"
		],
		"typo3cms:database:export": [
			".build/bin/typo3cms database:export -c Default -e 'cf_*' -e 'cache_*' -e '[bf]e_sessions' -e sys_log | gzip -c > Build/site/db.sql.gz",
			"ls -l Build/site/db.sql.gz"
		],
		"typo3cms:database:import": [
			"gzip -cdk Build/site/db.sql.gz | .build/bin/typo3cms database:import"
		],
		"typo3cms:install:setup": [
			".build/bin/typo3cms install:setup --no-interaction --use-existing-database --database-host-name='db' --database-name='db' --database-user-name='db' --database-user-password='db' --admin-user-name='admin' --admin-password='admin123ADMIN:)' --site-name='user-pizpalue dev' --web-server-config='apache' --skip-extension-setup"
		]
	}
}
